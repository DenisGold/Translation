{
  "name": "denisgold/translation-module",
  "description": "Module containing all AsgardCms translations by Denis Stoliarchuk",
  "type": "asgard-module",
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "composer/installers": "~1.0",
    "league/csv": "~7.2.0",
    "venturecraft/revisionable": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "3.5.*"
  },
  "autoload": {
    "psr-4": {
      "Modules\\Translation\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Translation\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "authors": [
    {
      "name": "Denis Stoliarchuk",
      "email": "dartius@mail.ru",
      "role": "Developer"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
